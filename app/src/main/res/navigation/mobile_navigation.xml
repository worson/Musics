<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/mobile_navigation"
  app:startDestination="@+id/navigation_home">

  <fragment
    android:id="@+id/navigation_home"
    android:name="app.sen.musics.ui.home.HomeFragment"
    android:label="@string/title_home"
    tools:layout="@layout/fragment_home" />

  <fragment
    android:id="@+id/navigation_musics"
    android:name="app.sen.musics.ui.list.MusicFragment"
    android:label="@string/title_dashboard"
    tools:layout="@layout/fragment_music_list" />

  <fragment
    android:id="@+id/navigation_settings"
    android:name="app.sen.musics.ui.settings.SettingsFragment"
    android:label="@string/title_notifications"
    tools:layout="@layout/fragment_notifications" />
</navigation>